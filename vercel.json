{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "node_service/Ping.Pub-explorer/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/node_service/Ping.Pub-explorer/(.*)",
      "dest": "/node_service/Ping.Pub-explorer/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
